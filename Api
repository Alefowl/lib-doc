  case GET -> Root / "languages" =>  Находит все языки
  case GET -> Root / "author" / IntVar(id) => находит автора по ID
  case GET -> Root / "authors" => Находит всех авторов
  case GET -> Root / "chapters" / IntVar(id) => Находит все главы книги по ID книги
  case GET -> Root / "book" / IntVar(id) / "language" / lang => Находит книгу по ID и ее перевод по Id языка
  case GET -> Root / "book" / IntVar(bId) / "chapter" / IntVar(chId) =>Находит книгу по ID и ее Главу по ID главы
  case GET -> Root / "book" / IntVar(id) => Находит книгу по ID
  case GET -> Root / "books" / IntVar(author) => Находит все книги автора по ID автора
  case GET -> Root / "bookF" / IntVar(id) => Находит кнгиу по ID ее главы и содержание
  case GET -> Root / "metabookF" / IntVar(bookId) => Находит книгу и ее переводы
  case GET -> Root / "metabookF2" / IntVar(metabookId) => Находит книгу и ее переводы
  case GET -> Root / "metabooks"  => Находит все книги в оригинале
  case GET -> Root / "version" => Показывает версию
  case GET -> Root / "booksize" / IntVar(book) => Находит количество глав книги по ID
  case GET -> Root / "notes" / IntVar(book) => Находит сноски в главах книги по ID 
  case GET -> Root / "tags" / IntVar(metabook) => Находит коментарии пользователя
  case GET -> Root / "pdf" / IntVar(book) => Показывает книгу в формате PDF
  case GET -> Root / "pdf2" / IntVar(book1) / IntVar(book2) => показывает 2 книги в формате PFD
  case request@POST -> Root / "auth" => Аунтификация пользователя
  case request@POST -> Root / "admin" / "insertBook" => Добавить книгу в переводе
  case request@POST -> Root / "admin" / "insertNotes" => Добавить сноску
  case request@POST -> Root / "admin" / "insertCreator" => Добавить автора
  case request@POST -> Root / "admin" / "insertMetabook" => Добавить книгу в оригинале
  case request@DELETE -> Root / "admin" / "deleteCreator" / Удалить автора
  case request@DELETE -> Root / "admin" / "deleteMetabook" / IntVar(metabook) => Удалить книгу в оригинале
  case request@DELETE -> Root / "admin" / "deleteBook" / IntVar(book) => Удалить перевод книги
  case request@GET -> Root / "admin" / "creators" => Находит авторов доступных пользователю
  case request@GET -> Root / "admin" / "books" => Находит бибилотеку пользователя
  case request@GET -> Root / "admin" / "metabooks" => Находит доступные книги для пользователя
  case request@POST -> Root / "admin" / "insertChapter" => Добавить заголовки
  case request@POST -> Root / "admin" / "raw" => Добавить запись в загрузки
